struct Token {
  lexeme: string,
}

struct Lexer {
  position: int,
  tokens: any, // Vec<Token>
}

fn (self: Lexer) run(input: string) {
  self.position = 0;
  self.tokens = _vec();

  loop {
    if self.position.eq(input.len()) {
      return self.tokens;
    };

    let token = input.slice(self.position, self.position.add(1));
    self.tokens.push(token);
  };
}

fn main() {
  let lexer = Lexer { position: 0, tokens: _vec() };

  _print(lexer.run("let a = 10;"));
}
