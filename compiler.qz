struct Lexer {
  input: string,
  position: int,
}

func (l: Lexer) run() {
  while (l.position < l.input.len()) {
    l.position = l.position + 1;
    _println(show_int(l.position));
  };
}

func main() {
  let input = "(module (func $main 0 (return 10)))";
  let lexer = Lexer {
    input: input, 
    position: 0,
  };
  lexer.run();

  _println(input);
}
