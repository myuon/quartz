struct Token {
  lexeme: string,
}

struct Lexer {
  position: int,
  tokens: any, // Vec<Token>
}

fn (self: Lexer) run(input: string) {
  self.position = 0;
  self.tokens = _vec();

  loop {
    let token = self.next_token(input);
    if token.lexeme == "" {
      break;
    }
    self.tokens.push(token);
  };

  return self.tokens;
}

fn main() {
  let lexer = Lexer { position: 0, tokens: _vec() };

  _print(lexer.run("let a = 10;"));
}
